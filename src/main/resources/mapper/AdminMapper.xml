<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manage.mapper.AdminMapper">
    <select id="selectAll" resultType="com.manage.domain.Admin">
        select * from admin
        <where>
            <if test="name != null">name like concat('%',#{name},'%')</if>
        </where>
        order by id desc
    </select>
    <insert id="insert" parameterType="com.manage.domain.Admin">
        insert into `admin` (username,password,role,name)
        values (#{username},#{password},#{role},#{name});
    </insert>
    <update id="updateById" parameterType="com.manage.domain.Admin">
        update `admin`
        set username = #{username},password = #{password},role = #{role},name = #{name}
        where id = #{id}
    </update>
</mapper>